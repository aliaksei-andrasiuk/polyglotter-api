generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PhraseMapping {
  phraseId     String  @id
  wordId       String
  sourceRoot   String  @db.VarChar(100)
  sourcePrefix String? @db.VarChar(100)
  sourceWord   String  @db.VarChar(100)
  targetPhrase String  @db.VarChar(255)
  wordsCount   Int     @db.SmallInt
}
